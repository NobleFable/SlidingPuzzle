            <ColumnDefinition Width="6"/>
        </Grid.ColumnDefinitions>
        
        <Rectangle Grid.Column="0" Grid.ColumnSpan="11" Grid.Row="0" Grid.RowSpan="12" Fill="DarkGreen" />
